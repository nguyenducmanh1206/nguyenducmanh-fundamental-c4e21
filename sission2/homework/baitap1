a =int(input("chieu cao cua ban la? "))
b = a/100
print(a, "(cm) = " , b , " (m)")
c =int(input("can nang cua ban la? "))
print(c, "(kg)")
d = b/(a*a)
if d<16:
    print("giam can nang")
elif 16 <= d <= 18.5:
    print("thieu can nang")
elif 18.5<=25:
    print("binh thuong")
elif 25<d<=30:
    print("thua can nang")
elif d>30:
    print("beo phi")










